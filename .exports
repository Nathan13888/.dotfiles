export PERS="/mnt/nas/personal"
export NOTE="$PERS/!Notes"
export MEDI="/mnt/nas/media"
export BACK="/mnt/nas/backup"
export ARCH="/mnt/nas/archival"
export WORK="~/workspace"
export CFTP="~/SFTP"
export CGIT="~/workspace/Git"
export DMOJ="~/workspace/Git/DMOJ"
export CHES="~/chess"
export DOTF="~/workspace/Git/DotFiles"
